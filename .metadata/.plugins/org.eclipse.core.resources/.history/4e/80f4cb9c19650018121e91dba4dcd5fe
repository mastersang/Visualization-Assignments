package infovis.scatterplot;

import java.awt.geom.Rectangle2D;
import java.util.ArrayList;

public class MatrixCell {
	private Rectangle2D cell;
	private ArrayList<PlotPoint> pointList;

	public MatrixCell(Rectangle2D cell, ArrayList<PlotPoint> pointList) {
		super();
		this.cell = cell;
		this.pointList = pointList;
	}

	public Rectangle2D getCell() {
		return cell;
	}

	public void setCell(Rectangle2D cell) {
		this.cell = cell;
	}

	public ArrayList<PlotPoint> getPointList() {
		return pointList;
	}

	public void setPointList(ArrayList<PlotPoint> pointList) {
		this.pointList = pointList;
	}
}
