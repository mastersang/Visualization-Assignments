package infovis.paracoords;

public class Axis {
	private int x;
	private double min;
	private double max;
	private double step;

	public Axis(int x, double min, double step) {
		this.x = x;
		this.min = min;
		this.step = step;
	}

	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public double getMin() {
		return min;
	}

	public void setMin(double min) {
		this.min = min;
	}

	public double getStep() {
		return step;
	}

	public void setStep(double step) {
		this.step = step;
	}
}